{
  "name": "@nfq/react-grid",
  "version": "2.2.2",
  "description": "An fork of react-awesome-styled-grid with more screen classes and some features.",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "jsnext:main": "dist/esm/index.js",
  "types": "types/index.d.ts",
  "engines": {
    "node": ">=14.0.0"
  },
  "standard-version": {
    "scripts": {
      "prebump": "nfqBump"
    }
  },
  "sideEffects": [
    "cypress/**"
  ],
  "scripts": {
    "build": "rollup -c",
    "deploy": "standard-version && git push --follow-tags origin main && npm publish",
    "lint": "eslint ./src --ext='.js, .jsx'",
    "lint:report": "eslint --output-file eslint_report.json --format json --ext='.js, .jsx' ./src",
    "prepublishOnly": "pnpm run build",
    "test": "pnpm cross-env NODE_ENV=test pnpm cypress run-ct",
    "test:open": "pnpm cross-env NODE_ENV=test pnpm cypress open-ct"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nfqde/nfq-react-grid.git"
  },
  "keywords": [
    "styled-components",
    "react",
    "ssr",
    "css-grid",
    "layout"
  ],
  "author": ".NFQ | Christoph Kruppe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nfqde/nfq-react-grid/issues"
  },
  "homepage": "https://github.com/nfqde/nfq-react-grid#readme",
  "peerDependencies": {
    "prop-types": ">= 15",
    "react": ">= 17",
    "styled-components": ">= 5"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-decorators": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@cypress/code-coverage": "^3.10.0",
    "@cypress/react": "^5.12.5",
    "@cypress/webpack-dev-server": "^1.8.4",
    "@nfq/eslint-config": "2.1.5",
    "@nfq/eslint-plugin": "^0.6.0",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-plugin-styled-components": "^2.0.7",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "cypress": "^9.7.0",
    "cypress-mochawesome-reporter": "^3.1.0",
    "cypress-real-events": "^1.7.0",
    "eslint": "^8.16.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-no-unsanitized": "^4.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-perf-standard": "^1.0.3",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-react-hooks-ssr": "^0.1.5",
    "eslint-plugin-redos": "^4.4.0",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "html-webpack-plugin": "^5.5.0",
    "mocha": "^10.0.0",
    "mochawesome": "^7.1.3",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-is": "^18.1.0",
    "rollup": "^2.75.3",
    "rollup-plugin-cleaner": "^1.0.0",
    "script-ext-html-webpack-plugin": "^2.1.5",
    "styled-components": "^5.3.5",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0"
  }
}
