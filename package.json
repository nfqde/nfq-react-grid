{
  "name": "@nfq/react-grid",
  "version": "1.1.1",
  "description": "An fork of react-awesome-styled-grid with more screen classes and some features.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "source": "src/index.jsx",
  "types": "types/index.d.ts",
  "engines": {
    "node": ">=14.0.0"
  },
  "standard-version": {
    "scripts": {
      "prebump": "nfqBump"
    }
  },
  "sideEffects": [
    "cypress/**"
  ],
  "scripts": {
    "build": "cross-env BABEL_ENV=production babel src --out-dir dist --copy-files",
    "deploy": "standard-version && git push --follow-tags origin main && npm publish",
    "lint": "eslint ./src --ext='.js, .jsx'",
    "lint:report": "eslint --output-file eslint_report.json --format json --ext='.js, .jsx' ./src",
    "prepare": "pnpm run build",
    "test": "pnpx cross-env NODE_ENV=test pnpx cypress run-ct",
    "test:open": "pnpx cross-env NODE_ENV=test pnpx cypress open-ct"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nfqde/nfq-react-grid.git"
  },
  "keywords": [
    "styled-components",
    "react",
    "ssr",
    "css-grid",
    "layout"
  ],
  "author": ".NFQ | Christoph Kruppe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nfqde/nfq-react-grid/issues"
  },
  "homepage": "https://github.com/nfqde/nfq-react-grid#readme",
  "peerDependencies": {
    "core-decorators": ">= 0.20",
    "prop-types": ">= 15",
    "react": ">= 17",
    "styled-components": ">= 5"
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/eslint-parser": "7.14.7",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-proposal-decorators": "7.14.5",
    "@babel/preset-env": "7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@cypress/code-coverage": "^3.9.10",
    "@cypress/react": "^5.9.4",
    "@cypress/webpack-dev-server": "^1.5.0",
    "@nfq/eslint-config": "1.2.2",
    "@nfq/eslint-plugin": "0.5.5",
    "babel-loader": "^8.2.2",
    "babel-plugin-add-module-exports": "1.0.4",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-preset-minify": "0.5.1",
    "clean-webpack-plugin": "^4.0.0",
    "core-decorators": "^0.20.0",
    "cross-env": "7.0.3",
    "cypress": "^8.3.1",
    "cypress-mochawesome-reporter": "^2.2.1",
    "cypress-real-events": "^1.5.1",
    "eslint": "7.30.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-array-func": "3.1.7",
    "eslint-plugin-better-styled-components": "1.1.2",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-no-unsanitized": "3.1.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-perf-standard": "1.0.3",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-sort-destructure-keys": "1.3.5",
    "html-webpack-plugin": "^5.3.2",
    "mochawesome": "^6.2.2",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "prop-types": "^15.7.2",
    "react": "17.0.2",
    "react-dom": "^17.0.2",
    "script-ext-html-webpack-plugin": "^2.1.5",
    "styled-components": "5.3.0",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.1.0"
  }
}
