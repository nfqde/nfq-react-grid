{
  "name": "@nfq/react-grid",
  "version": "1.0.1",
  "description": "An fork of react-awesome-styled-grid with more screen classes and some features.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "source": "src/index.jsx",
  "types": "types/index.d.ts",
  "engines": {
    "node": ">=14.0.0"
  },
  "standard-version": {
    "scripts": {
      "prebump": "nfqBump"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "cross-env BABEL_ENV=production babel src --out-dir dist --copy-files",
    "deploy": "standard-version && git push --follow-tags origin main && npm publish",
    "prepare": "pnpm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nfqde/nfq-react-grid.git"
  },
  "keywords": [
    "styled-components",
    "react",
    "ssr",
    "css-grid",
    "layout"
  ],
  "author": ".NFQ | Christoph Kruppe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nfqde/nfq-react-grid/issues"
  },
  "homepage": "https://github.com/nfqde/nfq-react-grid#readme",
  "peerDependencies": {
    "core-decorators": ">= 0.20",
    "prop-types": ">= 15",
    "react": ">= 17",
    "styled-components": ">= 5"
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/eslint-parser": "7.14.7",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-proposal-decorators": "7.14.5",
    "@babel/preset-env": "7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@nfq/eslint-config": "1.2.2",
    "@nfq/eslint-plugin": "0.5.5",
    "babel-plugin-add-module-exports": "1.0.4",
    "babel-preset-minify": "0.5.1",
    "core-decorators": "^0.20.0",
    "cross-env": "7.0.3",
    "eslint": "7.30.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-array-func": "3.1.7",
    "eslint-plugin-better-styled-components": "1.1.2",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-no-unsanitized": "3.1.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-perf-standard": "1.0.3",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-sort-destructure-keys": "1.3.5",
    "prop-types": "^15.7.2",
    "react": "17.0.2",
    "styled-components": "5.3.0"
  }
}
